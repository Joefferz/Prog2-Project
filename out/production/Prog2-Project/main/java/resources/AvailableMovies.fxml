<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.movierental.test.AvailableMoviesController">
   <children>
      <BorderPane layoutY="8.0" prefHeight="400.0" prefWidth="600.0">
         <top>
            <Label text="LIST OF MOVIES" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="30.0" top="40.0" />
               </BorderPane.margin>
               <font>
                  <Font name="Impact" size="18.0" />
               </font>
            </Label>
         </top>
         <center>
            <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="colID" prefWidth="103.0" text="MOVIE ID" />
                <TableColumn fx:id="colName" prefWidth="496.0" text="MOVIE NAME" />
              </columns>
               <BorderPane.margin>
                  <Insets left="20.0" right="20.0" />
               </BorderPane.margin>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
